pipeline {
    agent any

    stages { 
        stage ("start docker container") { 
            steps {
                echo " =============== start docker container ================="
                dir ('project_jenkins/') {
                    sh 'docker run -v /www/mysite:/usr/share/nginx/html:ro -p 9889:80 nginx:stable'
                }
            }
        }
    }
}
